# ResumYT - YouTube Video Summarization Platform
# Copy this file to .env and fill in your actual API keys and configuration

# DeepSeek API (for AI text processing)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# YouTube Data API v3 (for video metadata)
NEXT_PUBLIC_YOUTUBE_API_KEY=your_youtube_api_key_here
YOUTUBE_API_KEY=your_youtube_api_key_here

# OpenAI API (for text generation)
OPENAI_API_KEY=your_openai_api_key_here

# OpenRouter API (alternative AI provider)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Replicate API (for additional AI models)
REPLICATE_API_TOKEN=your_replicate_token_here

# Google OAuth Configuration
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Application URLs
NEXT_PUBLIC_BASE_URL=https://your-domain.com
NEXT_PUBLIC_APP_URL=http://localhost:3000

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# Instructions:
# 1. Copy this file to .env: cp .env.example .env
# 2. Replace all placeholder values with your actual API keys
# 3. Never commit .env to version control
# 4. Keep your API keys secure and private